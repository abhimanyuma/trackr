<% if @company.errors.any? %>
	<div id="error-explanation">
		<div class="alert alert-error">
			This form contains <%= pluralize(@company.errors.count, "error") %>.
		
		<ul>
			<% @company.errors.full_messages.each do |msg| %>
				<li > <%= msg %> </li>
			<% end %>
		</ul>
		</div>
	</div>
<% end %>