<div class="btn-group ">
  <%= link_to "Contact", contact, :class => "btn btn-primary" %>
  <button class="btn btn-primary dropdown-toggle" data-toggle="dropdown">
    <span class="caret"></span>
  </button>
  <ul class="dropdown-menu">
    <% if current_user.superadmin?  %>
      <li><%= link_to "Permanently Delete User",contact,method: :delete, data: {confirm: "This will permanently delete the user, the action CANNOT be undone, are you sure ?"} %></li>
    <% end %>
  </ul>
</div>